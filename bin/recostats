#!/usr/bin/env ruby

sentences_count = 0
max_words       = 0
users           = {}

STDIN.each_line do |line|
  sentences_count += 1
  words = line.split(' ')
  words.each do |user|
    users[user] = true
  end
  max_words = words.size unless words.size < max_words
end

p [:sentences_count, sentences_count]
p [:max_words, max_words]
p [:users, users.size]
